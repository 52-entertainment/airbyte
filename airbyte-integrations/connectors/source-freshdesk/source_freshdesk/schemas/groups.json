{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "escalate_to": {
      "type": ["integer", "null"]
    },
    "unassigned_for": {
      "type": ["string", "null"]
    },
    "business_hour_id": {
      "type": ["integer", "null"]
    },
    "group_type": {
      "type": "string"
    },
    "created_at": {
      "format": "date-time",
      "type": "string"
    },
    "updated_at": {
      "format": "date-time",
      "type": "string"
    },
    "auto_ticket_assign": {
      "type": "boolean"
    }
  }
}
