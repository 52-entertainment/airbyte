{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "note": {
      "type": ["string", "null"]
    },
    "domains": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "created_at": {
      "format": "date-time",
      "type": "string"
    },
    "updated_at": {
      "format": "date-time",
      "type": "string"
    },
    "custom_fields": {
      "type": "object"
    },
    "health_score": {
      "type": ["string", "null"]
    },
    "account_tier": {
      "type": "string"
    },
    "renewal_date": {
      "format": "date-time",
      "type": ["string", "null"]
    },
    "industry": {
      "type": ["string", "null"]
    }
  }
}
