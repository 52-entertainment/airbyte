  "type": "object",
  "properties": {
    "billable": {
      "type": "boolean"
    },
    "note": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "timer_running": {
      "type": "boolean"
    },
    "agent_id": {
      "type": "integer"
    },
    "ticket_id": {
      "type": "integer"
    },
    "company_id": {
      "type": ["integer", "null"]
    },
    "time_spent": {
      "type": "string"
    },
    "executed_at": {
      "format": "date-time",
      "type": "string"
    },
    "start_time": {
      "format": "date-time",
      "type": "string"
    },
    "created_at": {
      "format": "date-time",
      "type": "string"
    },
    "updated_at": {
      "format": "date-time",
      "type": "string"
    }
  }
}
