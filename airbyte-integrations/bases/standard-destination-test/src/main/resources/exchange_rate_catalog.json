{
  "streams": [
    {
      "stream": {
        "name": "exchange_rate",
        "json_schema": {
          "properties": {
            "id": {
              "type": "integer"
            },
            "currency": {
              "type": "string"
            },
            "date": {
              "type": "string"
            },
            "HKD": {
              "type": "number"
            },
            "NZD": {
              "type": "number"
            },
            "USD": {
              "type": "number"
            }
          }
        }
      },
      "sync_mode": "full_refresh",
      "destination_sync_mode": "overwrite"
    }
  ]
}
